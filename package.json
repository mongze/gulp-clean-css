{
  "name": "@aptuitiv/gulp-clean-css",
  "description": "Minify css with clean-css.",
  "homepage": "https://github.com/aptuitiv/gulp-clean-css",
  "version": "4.5.3",
  "author": "scniro and aptuitiv",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aptuitiv/gulp-clean-css/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aptuitiv/gulp-clean-css.git"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "css",
    "clean",
    "minify",
    "uglify",
    "clean-css",
    "minify-css",
    "gulp-minify-css",
    "gulp-clean-css",
    "gulpplugin",
    "gulpfriendly"
  ],
  "nyc": {
